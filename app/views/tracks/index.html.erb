<h1>Listing tracks</h1>

<table>
  <tr>
    <th>Album</th>
    <th>Artist</th>
    <th>Track</th>
    <th>Album art url</th>
    <th>Track mp3 url</th>
	<th>Edit</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tracks.each do |track| %>
  <tr>
    <td><%= track.album %></td>
    <td><%= track.artist %></td>
    <td><%= track.track %></td>
    <td><%= track.album_art_url %></td>
    <td><%= track.track_mp3_url %></td>
    <td><%= link_to 'Show', track %></td>
    <td><%= link_to 'Edit', edit_track_path(track) %></td>
    <td><%= link_to 'Destroy', track, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Track', new_track_path %>
