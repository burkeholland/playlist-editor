<h1>Listing playlists</h1>

<table>
  <tr>
    <th>Album</th>
    <th>Artist</th>
    <th>Album art url</th>
    <th>Track audio url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @playlists.each do |playlist| %>
  <tr>
    <td><%= playlist.album %></td>
    <td><%= playlist.artist %></td>
    <td><%= playlist.album_art_url %></td>
    <td><%= playlist.track_audio_url %></td>
    <td><%= link_to 'Show', playlist %></td>
    <td><%= link_to 'Edit', edit_playlist_path(playlist) %></td>
    <td><%= link_to 'Destroy', playlist, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Playlist', new_playlist_path %>
